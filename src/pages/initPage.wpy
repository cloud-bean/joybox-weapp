<style lang="less" scoped>

</style>
<template>
  <view class="">
    loading
  </view>
</template>

<script>
  import wepy from 'wepy'
  import * as api from '../api/user'
  export default class bindPage extends wepy.page {
    data = {
      inputContent: {},
      formVisable: false
    }
    async onLoad() {
      wepy.showLoading({
        title: '加载中'
      })
      const loginInfo = await api.login()
      wepy.hideLoading()
      if (loginInfo.code === 'success') {
        wepy.reLaunch({
          url: 'index'
        })
      } else {
        wepy.reLaunch({
          url: 'bindPage'
        })
      }
    }
  }

</script>
